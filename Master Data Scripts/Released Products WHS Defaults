CREATE Procedure ReleasedProductsWHSDefaults_GPToD365DB as
Insert into [dbo].[ProductsWHSDefault](
	DataAreaId,
	[ItemNumber] ,
	CountingGroupId
	)
Select 'GICT',
i.ITEMNMBR,
USCATVLS_1 as CountingGroupId
from gwc.dbo.IV00101  i
WHERE I.ITEMTYPE = 1
AND I.ITMCLSCD != 'Delete'
and i.ITEMNMBR in (select p.item from POSDatabase.[SiriusSQL].[dbo].[items] p)



