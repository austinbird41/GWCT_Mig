ALTER Procedure [dbo].[OnHandInventory_GPToD365DB]
as

INSERT INTO [dbo].[OnHandInventory]
           ([DataAreaId]
           ,[journalnumber]
           ,[LineNumber],
		   itemnumber
           ,[INVENTORYSITEID]
           ,[INVENTORYWAREHOUSEID]
           ,[WarehouseLocationID]
           ,[InventoryQuantity]
           ,[JournalNameId],
		   UnitCost, 
		   DEFAULTLEDGERDIMENSIONDISPLAYVALUE,
		   OFFSETMAINACCOUNTIDDISPLAYVALUE)
	 (
select 
	'GICT',
	case 
		when i2.LOCNCODE like 'new%'
			then (select top 1 journalnumber from onhandinventoryjournalheaders where DefaultWAREHOUSEID = '100')
		else  (select top 1 journalnumber from onhandinventoryjournalheaders where defaultwarehouseid=i2.LOCNCODE) 
	end AS JournalNumber,
	'' as LineNumber,
	i2.ITEMNMBR as ItemNumber, 
	'10' AS INVENTORYSITEID, 
		case
		when i2.LOCNCODE like 'new%'
			then '100'
		else i2.LOCNCODE
	end as INVENTORYWAREHOUSEID,
    '' as WarehouseLocationID,
    sum(i2.QTYONHND) as InventoryQuantity,
	'IMOV' as JournalNameId,
	i1.CURRCOST as UnitCost
	--'' as LicensePlateNumber,
	--,gl5.ACTNUMST --DefaultledgerDimensionDisplayValue
	,case
		when i2.LOCNCODE like 'new%'
			then'-160000-9999-1-Proj0000' 
		else Concat('-',trim(i2.LOCNCODE), '00','-9999-1-Proj0000') 
	end as DefaultledgerDimensionDisplayValue
	,Case
		when USCATVLS_2 = 'Halloween'
			then '13102'
		ELSE '13101'
	end as OFFSETMAINACCOUNTIDDISPLAYVALUE
	--map department value from warehouse, typcode will be 1, fundingsource will be 9999, project proj000
from gwc.dbo.IV00102 i2
join gwc.dbo.iv00101 i1
on i1.ITEMNMBR = i2.ITEMNMBR
join gwc.dbo.GL00105 gl5
on gl5.ACTINDX = i1.IVIVINDX
where i1.ITEMTYPE = 1
AND I1.ITMCLSCD != 'Delete'
and i2.QTYONHND != 0
and i2.RCRDTYPE = 2
and ACTNUMST like '180%'
and i2.LOCNCODE !='1102'
group by i2.ITEMNMBR, i2.LOCNCODE, i1.CURRCOST, USCATVLS_2
having sum(i2.QTYONHND) > 0

)