USE [D365Migration]
GO
/****** Object:  StoredProcedure [dbo].[CustomerContacts_GPTOD365DB]    Script Date: 3/18/2022 2:31:17 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
ALTER PROCEDURE [dbo].[CustomerContacts_GPTOD365DB]
AS

INSERT INTO [dbo].[CUSTOMERCONTACTS]
           ([Locator]
           ,[ISPRIMARY]
           ,[Type]
           ,[PartyNumber])

select 

left(rm2.PHONE1,10) as Locator,
1 as ISPRIMARY,
'Phone' as Type,
rm1.partynumber as PartyNumber
from gwc.dbo.RM00102 rm2
 join D365Migration.dbo.Customers rm1
 on rm1.salesaccountnumber = rm2.CUSTNMBR
 where rm2.phone1 != ''
 and rm2.phone1 not like '0%'

Union

select 

left(rm2.PHONE2,10) as Locator,
0 as ISPRIMARY,
'Phone' as Type,
rm1.partynumber
from gwc.dbo.RM00102 rm2
 join d365migration.dbo.customers rm1
 on rm1.salesaccountnumber = rm2.CUSTNMBR
 where 
 rm2.phone2  != ''
  and rm2.phone2 not like '0%'

 Union

select 

left(rm2.PHONE3,10) as Locator,
0 as ISPRIMARY,
'Phone' as Type,
rm1.partynumber
from gwc.dbo.RM00102 rm2
 join d365migration.dbo.customers rm1
 on rm1.salesaccountnumber = rm2.CUSTNMBR
 where 
 rm2.phone3  != ''
  and rm2.phone3 not like '0%'


Union

select 

left(rm2.FAX,10) as Locator,
1 as ISPRIMARY,
'Fax' as Type,
rm1.partynumber
from gwc.dbo.RM00102 rm2
 join d365migration.dbo.customers rm1
 on rm1.salesaccountnumber = rm2.CUSTNMBR
 where 
 rm2.fax != ''
  and rm2.fax not like '0%'

  UNION

